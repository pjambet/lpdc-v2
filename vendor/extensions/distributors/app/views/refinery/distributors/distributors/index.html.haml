%article#distributors
  != @page.content_for(:body)
  %ul
    - @distributors.each do |distributor|
      %li.distributor
        %h3= link_to distributor.name, distributor.website
        %p!= [distributor.distributor_type, distributor.address].select{|i| i.present?}.join(' - ')

- content_for :aside_content do
  != @page.content_for(:side_body)
