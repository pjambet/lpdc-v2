%article
  %article
    != @page.content_for(:body)
  %ul#products
    - @products.each do |product|
      %li
        = product.title
        = product.price
        = image_tag product.photo.url, width: '550'
        != product.blurb

- content_for :aside_content do
  != @page.content_for(:side_body)

